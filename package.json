{
  "name": "noted",
  "version": "1.0.0",
  "private": true,
  "description": "Noted by Zach Donnelly",
  "author": "Zachary Donnelly",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZacharyDonnelly/Noted"
  },
  "keywords": [
    "Next.js",
    "Next 14",
    "Next-Auth",
    "React",
    "React server components",
    "React client components",
    "React hooks",
    "TypeScript",
    "Jest",
    "SASS",
    "Prisma",
    "Cypress",
    "Docker",
    "sqlite",
    "sqlite3",
    "classnames",
    "Google",
    "Github",
    "JavaScript",
    "Noted",
    "Blog",
    "Personal Blog",
    "Real-Time Transcription"
  ],
  "scripts": {
    "audit": "yarn npm audit",
    "build": "next build",
    "build:server": "npm-run-all --parallel build typecheck:server",
    "clean": "rimraf public .cache",
    "cy:open": "cypress open",
    "cy:open:dev": "CYPRESS_STAGE=dev CYPRESS_BASE_URL=http://localhost:8000 cypress open",
    "cy:run": "cypress run",
    "dev": "next dev",
    "dev:server": "nodemon --config ./config/server/nodemon.json",
    "format:all": "npm-run-all format:js format:styles",
    "format:js": "prettier 'src/app/*.{js,jsx,ts,tsx}' --write",
    "format:styles": "prettier 'src/app/styles/*.css' --write",
    "lint:all": "npm-run-all lint:ts lint:styles lint:format",
    "lint:deadcode": "ts-prune -i '(tests)'",
    "lint:format": "prettier --check 'src/app/*.{js,jsx,ts,tsx,css}'",
    "lint:md": "markdownlint-cli2 **/*.{md,mdx}",
    "lint:styles": "stylelint src/app/styles/*.{scss,css}",
    "lint:ts": "eslint --ext .ts,.tsx,.js, .",
    "lint:ts:fix": "eslint --ext .ts,.tsx,.js,.jsx --fix .",
    "prepare": "node .husky/install.mjs",
    "prisma:studio": "npx prisma studio",
    "prisma:test": "npx ts-node --esm ./src/app/utils/prisma/script.ts",
    "start": "next start",
    "start:server": "cross-env NODE_ENV=production node dist/server.js",
    "test": "node --experimental-vm-modules $(yarn bin jest)",
    "test:e2e": "npm-run-all CYPRESS_SUPPORT=y build start-server-and-test serve http://localhost:9000 cy:open",
    "test:e2e:ci": "npm-run-all CYPRESS_SUPPORT=y build start-server-and-test serve http://localhost:9000 cy:run",
    "test:e2e:dev": "CYPRESS_SUPPORT=y CYPRESS_BASE_URL=http://localhost:8000 CYPRESS_STAGE=dev start-server-and-test develop http://localhost:8000 cy:open",
    "typecheck": "tsc --noEmit",
    "typecheck:server": "tsc --noEmit -p ./config/server/tsconfig.server.json",
    "typecheck:watch": "yarn type-check -- --watch",
    "upgrade": "yarn up -i"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/mdx": "^14.1.0",
    "@prisma/client": "^5.9.1",
    "bcryptjs": "^2.4.3",
    "next": "latest",
    "next-auth": "^4.24.6",
    "prisma": "^5.9.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.50.1",
    "swr": "^2.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@next/eslint-plugin-next": "^14.1.0",
    "@testing-library/cypress": "^10.0.1",
    "@testing-library/dom": "^9.3.4",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/mdx": "^2.0.11",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "babel-jest": "^29.7.0",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.4",
    "cypress-axe": "^1.5.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "latest",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "latest",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "latest",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^9.0.10",
    "ioredis": "^5.3.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "markdownlint-cli2": "^0.12.1",
    "nodemon": "^3.0.3",
    "npm-run-all2": "^6.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "sass": "^1.70.0",
    "scriptlint": "^3.0.0",
    "stylelint": "^16.2.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-prettier": "^5.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typescript": "^5.3.3"
  },
  "browserslist": [
    ">0.2%",
    "defaults",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "browser": {
    "crypto": false
  },
  "engines": {
    "node": ">=20"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:all && yarn typecheck"
    }
  },
  "packageManager": "yarn@4.1.0",
  "resolutions": {
    "caniuse-lite": "1.0.30001669"
  },
  "scriptlint": {
    "strict": true,
    "rules": {
      "uses-allowed-namespace": false
    }
  }
}
